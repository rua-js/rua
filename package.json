{
  "name": "rua",
  "version": "0.13.3",
  "description": "Elegant data utilities for front-end applications",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest test.ts",
    "build": "rm -rf ./lib && tsc",
    "commit": "git-cz",
    "move-dts": "node ./scripts/release/move-dts.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rua-js/rua.git"
  },
  "keywords": [
    "rua",
    "rua.js",
    "elegant",
    "rapid"
  ],
  "author": "copydog",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rua-js/rua/issues"
  },
  "gitHooks": {
    "commit-msg": "node scripts/git/verify-commit-message.js"
  },
  "homepage": "https://github.com/rua-js/rua#readme",
  "dependencies": {
    "@types/seamless-immutable": "^7.1.2",
    "autobind-decorator": "^2.1.0",
    "bluebird": "^3.5.1",
    "dva": "^2.1.0",
    "dva-core": "*",
    "faker": "^4.1.0",
    "global": "^4.3.2",
    "i18n-js": "^3.0.3",
    "invariant": "^2.2.2",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.6",
    "lodash": "^4.17.10",
    "react-native-i18n": "^2.0.12",
    "rua-core": "^0.2.4",
    "seamless-immutable": "^7.1.3",
    "superagent": "^3.8.3",
    "wolfy87-eventemitter": "^5.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/parser": "^7.0.0-beta.51",
    "@babel/traverse": "^7.0.0-beta.51",
    "@types/bluebird": "^3.5.20",
    "@types/faker": "^4.1.2",
    "@types/fs-extra": "^5.0.1",
    "@types/i18n-js": "^3.0.0",
    "@types/invariant": "^2.2.29",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^22.1.1",
    "@types/lodash": "^4.14.108",
    "@types/node": "^9.4.6",
    "@types/react-native": "^0.55.12",
    "@types/react-native-i18n": "^2.0.0",
    "@types/superagent": "^3.8.0",
    "babel-core": "^6.26.3",
    "babel-types": "^6.26.0",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.1",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "fs-extra": "^5.0.0",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.3",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.7.1",
    "yorkie": "^1.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "setupFiles": [
      "./src/mocks/browserMocks.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  }
}
